#include "protheus.ch"
#include "RestFul.CH"
#include "topconn.ch"
#include "tbiconn.ch"
#include "tlpp-rest.th"

class UserController
    @Post("/login")
    public method Login()
endclass

method Login() class UserController
    Local cContent := Self:GetContent()
    Local aResponse := LoginService():Login(cContent)

    ::SetResponse(aResponse)
return

